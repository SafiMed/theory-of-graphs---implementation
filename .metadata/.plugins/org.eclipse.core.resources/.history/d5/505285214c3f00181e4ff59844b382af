public class ManageArcs extends JFrame {

	JTable table = new JTable(new DefaultTableModel(new Object[]{"Noeud ID", "Nom"}, 0));
	DefaultTableModel model = (DefaultTableModel) table.getModel();

	
	/**
	 * Create the frame.
	 */
	public ManageArcs() {
